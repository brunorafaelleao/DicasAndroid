<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ColunaPrincipal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".TelaInicial"
    tools:ignore="ExtraText">


    <ImageButton
        android:id="@+id/btConsultaCadastro"
        android:layout_width="60dp"
        android:layout_height="66dp"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:src="@drawable/iconepessoas"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btCadastro"
        android:layout_width="60dp"
        android:layout_height="66dp"
        android:layout_gravity="center"
        android:src="@drawable/iconecadastro"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />



    <TextView
            android:id="@+id/MsgInicial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="150dp"
            android:text="@string/msgInicial"/>

    <Button
            android:id="@+id/btSincronizar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/sincronizacao" />




</LinearLayout>